{
  "source": {
    "include": ["./src"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
